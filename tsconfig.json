{
  "compilerOptions": {
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "target": "ES2020",
      "module": "ES2020",
      "declaration": true,
      "isolatedModules": true,
      "moduleResolution": "Node",
      "noEmit": true,
      "outDir": "dist",
  },
  "include": ["src"],
  "exclude": ["node_modules", "spec"]
}